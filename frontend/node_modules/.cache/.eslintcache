 'name': name}\n  }\n\n  handleSubmit() {\n    // this.props.history.push(\"/Home\");\n    var details = this.retrieveSignUpDetails();\n  }\n\n  render() {\n    return (\n      <div className=\"SignUp\">\n        <h1>SignUp</h1>\n        <div className=\"SignUpBox\">\n          <div className=\"TextInput\">\n            <label>Username: </label>\n            <input type=\"text\" id = \"username\" />\n          </div>\n          <div className=\"TextInput\">\n            <label>Name: </label>\n            <input type=\"text\" id = \"name\" />\n          </div>\n          <div className=\"TextInput\">\n            <label>Email ID: </label>\n            <input type=\"text\" id = \"email\" />\n          </div>\n          <div className=\"TextInput\">\n            <label>Password: </label>\n            <input type=\"text\" id = \"password\" />\n          </div>\n          <input type=\"button\" value=\"Submit\" onClick={this.handleSubmit} />\n        </div>\n        <p>\n          Already a user?\n        </p>\n        <a href=\"/Login\">Login Here</a>\n      </div>\n    );\n  }\n}\n  \nexport default SignUp;","/home/osheen/sem7/fcs/fcs_project/frontend/src/Components/Home/Home.jsx",["55","56","57","58","59","60","61","62","63","64","65"],"import \"./Home.scss\"\nimport React from \"react\"\nimport { render } from \"@testing-library/react\"\nimport Products from \"./Products/Products\"\nimport { ShoppingBasket, ShoppingCart } from \"@material-ui/icons\";\nimport { IconButton } from \"@material-ui/core\";\n\n\nclass Home extends React.Component {\n\n  constructor(props) {\n    super(props);\n    this.categories = [\"All\", \"Electronics\", \"Household\", \"Sports\", \"Fashion\", \"Entertainment\"]\n    this.fetchProducts();\n    this.state = {\n      selectedCategory: this.categories[0],\n      products: [],\n      query: \"\",\n      filteredProducts: [],\n      cart: sessionStorage.getItem('cart') ? sessionStorage.getItem('cart').split(',') : [],\n      showCart: false\n    }\n  }\n\n  fetchProducts = () => {\n    var axios = require('axios');\n    axios.get('http://localhost:5000/products').then((response) => {\n      this.setState({...this.state, products: response.data, filteredProducts: response.data})\n    });\n  } \n\n  updateCategory = (event) => {\n    this.setState({...this.state, selectedCategory: event.target.value}, () => {\n      this.filterProducts();\n    })\n  }\n\n  filterProducts = () => {\n    var filtered = this.state.products.filter((product) => {\n      console.log(this.state.query == product.name.substring(0, this.state.query.length));\n      return (this.state.selectedCategory == 'All' || product.category == this.state.selectedCategory) && this.state.query == product.name.substring(0, this.state.query.length);\n    })\n    this.setState({filteredProducts: filtered})\n  }\n\n  handleSearchQueryChange = (event) => {\n    this.setState({...this.state, query: event.target.value}, () => {\n      this.filterProducts()\n    })\n  }\n\n  addToCart = (id) => {\n    sessionStorage.setItem('cart', [...this.state.cart, id])\n    this.setState({cart: [...this.state.cart, id]}, () => console.log(this.state.cart));\n  }\n\n  removeFromCart = (id) => {\n    var tempCart = []\n    var deleted = false;\n    for(var i in this.state.cart) {\n      if(id == this.state.cart[i].id || !deleted) {\n        deleted = true;\n        continue;\n      } else {\n        tempCart.push(this.state.cart[i])\n      }\n    }\n    sessionStorage.setItem('cart', tempCart)\n    this.setState({cart: tempCart}, () => console.log(this.state.cart));\n  }\n\n  render() {\n    return (\n      <div className=\"Home\">\n        <div className=\"Categories\">\n          {this.categories.map((category) => <input aria-selected={this.state.selectedCategory==category} type=\"button\" value={category} onClick={this.updateCategory} />)}\n        </div>\n        <div className=\"Search\">\n          <input value={this.state.query} onChange={this.handleSearchQueryChange} type=\"text\" className=\"Sarch\" placeholder=\"Search for product name, description etc\" />\n        </div>\n        <Products products={this.state.filteredProducts} addToCart={this.addToCart} removeFromCart={this.removeFromCart} />\n      </div>\n    );\n  }\n}\n\nexport default Home;\n","/home/osheen/sem7/fcs/fcs_project/frontend/src/Components/Header/Header.jsx",[],"/home/osheen/sem7/fcs/fcs_project/frontend/src/Components/Checkout/Checkout.jsx",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":10,"column":111,"nodeType":"72","messageId":"73","endLine":10,"endColumn":113},{"ruleId":"74","severity":1,"message":"75","line":27,"column":13,"nodeType":"76","endLine":27,"endColumn":76},{"ruleId":"70","severity":1,"message":"71","line":30,"column":31,"nodeType":"72","messageId":"73","endLine":30,"endColumn":33},{"ruleId":"77","severity":1,"message":"78","line":3,"column":10,"nodeType":"79","messageId":"80","endLine":3,"endColumn":20},{"ruleId":"77","severity":1,"message":"81","line":5,"column":8,"nodeType":"79","messageId":"80","endLine":5,"endColumn":26},{"ruleId":"82","severity":1,"message":"83","line":48,"column":29,"nodeType":"84","messageId":"85","endLine":48,"endColumn":30,"suggestions":"86"},{"ruleId":"82","severity":1,"message":"83","line":48,"column":45,"nodeType":"84","messageId":"85","endLine":48,"endColumn":46,"suggestions":"87"},{"ruleId":"77","severity":1,"message":"88","line":85,"column":9,"nodeType":"79","messageId":"80","endLine":85,"endColumn":16},{"ruleId":"77","severity":1,"message":"89","line":3,"column":10,"nodeType":"79","messageId":"80","endLine":3,"endColumn":16},{"ruleId":"77","severity":1,"message":"90","line":5,"column":10,"nodeType":"79","messageId":"80","endLine":5,"endColumn":24},{"ruleId":"77","severity":1,"message":"91","line":5,"column":26,"nodeType":"79","messageId":"80","endLine":5,"endColumn":38},{"ruleId":"77","severity":1,"message":"92","line":6,"col